<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>WOLI_Completed</fullName>
    <active>true</active>
    <errorConditionFormula>IF(  ISPICKVAL( PRIORVALUE(Status), &apos;Completed&apos;) , true, false)</errorConditionFormula>
    <errorMessage>No puede modificar un vez que ha sido completada</errorMessage>
</ValidationRule>
