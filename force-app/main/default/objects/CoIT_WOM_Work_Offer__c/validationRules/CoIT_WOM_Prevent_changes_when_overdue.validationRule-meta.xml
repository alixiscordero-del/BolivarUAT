<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CoIT_WOM_Prevent_changes_when_overdue</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL( CoIT_WOM_Status__c, &apos;Overdue&apos;) &amp;&amp; ISCHANGED(CoIT_WOM_Acceptance_Status__c)</errorConditionFormula>
    <errorMessage>Cannot change answer because offer is overdue</errorMessage>
</ValidationRule>
