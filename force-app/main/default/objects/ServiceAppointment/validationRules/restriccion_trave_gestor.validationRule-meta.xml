<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>restriccion_trave_gestor</fullName>
    <active>true</active>
    <description>Restriccion de cambio de estado &quot;travel&quot; para los gestores</description>
    <errorConditionFormula>$Profile.Name  = &quot;CoIT Bolivar Manager Profile&quot;  &amp;&amp;  ISPICKVAL(Status, &quot;Travel&quot;)</errorConditionFormula>
    <errorDisplayField>Status</errorDisplayField>
    <errorMessage>No tiene permisos para cambiar este estado</errorMessage>
</ValidationRule>
